@{
    Layout = null;
}
@model TransportScheduling.Models.T_Infor_Indent

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Add</title>
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
</head>
<body>
    <div>
        <form action="@Url.Action("Add","Indent")" method="post" name="FormAdd">
            <table>
                <tr><td>订单内容：<input type="text" name="IContent" /></td></tr>
                <tr><td>签订日期：<input type="text" name="ISigningDate" /></td></tr>
                <tr><td>截止时期：<input type="text" name="IExpirationDate" /></td></tr>
                <tr><td>状态：<input type="text" name="IState" /></td></tr>
                <tr><td>交付日期：<input type="text" name="IDeliveryDate" /></td></tr>
                <tr><td>备注：<input type="text" name="IRemarks" /></td></tr>
                <tr><td><input type="submit" value="添加" /></td></tr>
            </table>


        </form>
    </div>
</body>
</html>
